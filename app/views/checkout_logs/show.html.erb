<% provide(:title, 'Checkout History') %>

<h1>Checkout History</h1>
<ul class="checkout_logs">
  <% @checkout_logs.each do |checkout_log| %>
  <% if (checkout_log.book == @book.title) %>
    <li>
      <h4><%= checkout_log.user %> | <%= checkout_log.created_at %></h4>
      <% end %>
    </li>
  <% end %>
</ul>
